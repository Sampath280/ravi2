{
  "routes": [
    {
      "route": "/bakery",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/bakery/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/bakery/index.html"
  },
  "globalHeaders": {
    "cache-control": "no-store, must-revalidate"
  },
  "trailingSlash": "auto"
}
